var eventData =
{
  id: '<%= @event.id %>',
  title: '<%= @event.title %>',
  start: '<%= @event.start_time %>',
  end: '<%= @event.end_time.nil? ? @event.start_time : @event.end_time %>',
};
$('#calendar-content').fullCalendar('renderEvent', eventData, true); // stick? = true
$('#event-modal-container').foundation('reveal', 'close')