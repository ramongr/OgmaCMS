<h1>Listing admin_blogs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_blogs.each do |admin_blog| %>
      <tr>
        <td><%= admin_blog.title %></td>
        <td><%= admin_blog.content %></td>
        <td><%= admin_blog.user %></td>
        <td><%= admin_blog.date_created_human %></td>
        <td><%= admin_blog.date_updated_human %></td>
        <td><%= link_to 'Show', [:admin,admin_blog] %></td>
        <td><%= link_to 'Edit', edit_admin_blog_path(admin_blog) %></td>
        <td><%= link_to 'Destroy', [:admin,admin_blog], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blog', new_admin_blog_path %>
