<h1>Listing admin_galleries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @galleries.each do |gallery| %>
      <tr>
        <td><%= gallery.title %></td>
        <td><%= gallery.description %></td>
        <td><%= gallery.date %></td>
        <td><%= link_to 'Show', [:admin,gallery] %></td>
        <td><%= link_to 'Edit', edit_admin_gallery_path(gallery) %></td>
        <td><%= link_to 'Destroy', [:admin,gallery], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gallery', new_admin_gallery_path %>
